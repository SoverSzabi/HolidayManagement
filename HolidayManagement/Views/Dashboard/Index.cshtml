<div class="container">
    <h2>Dynamic Tabs</h2>
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#home">Home</a></li>
        <li><a data-toggle="tab" href="#menu1">Users</a></li>
        <li><a data-toggle="tab" href="#menu2">Group Managment</a></li>
        <li><a data-toggle="tab" href="#menu3">My Calendar</a></li>
        <li><a data-toggle="tab" href="#menu4">Settings</a></li>
    </ul>

    <div class="tab-content">
        <div id="home" class="tab-pane fade in active">
           <h2>Welcome </h2>
         </div>
        <div id="menu1" class="tab-pane fade">
          @Html.Action("Users", "Dashboard")
        </div>
        <div id="menu2" class="tab-pane fade">
            @Html.Action("GroupManagment", "Dashboard")    
         </div>
        <div id="menu3" class="tab-pane fade">
            @Html.Action("MyCalendar", "Dashboard")
         </div>
        <div id="menu4" class="tab-pane fade">
            @Html.Action("Settings", "Dashboard")
        </div>
    </div>
</div>

